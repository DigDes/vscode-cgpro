{
	"name": "CommuniGate Pro",
	"scopeName": "source.cgpro",
	"fileTypes": [
		"wcgp",
		"wcgi",
		"sppi",
		"sppr",
		"wssp",
		"wssi"
	],
	"patterns": [
		{
			"include": "text.html.basic"
		},
		{
			"comment": "Block comment",
			"name": "comment.line.block.cgpro",
			"begin": "/\\*",
			"end": "\\*/"
		},
		{
			"comment": "Line comment",
			"name": "comment.line.double-slash.cgpro",
			"begin": "//",
			"end": "$"
		},
		{
			"comment": "Decimal fraction",
			"name": "constant.numeric.cgpro",
			"match": "\\b([0-9]+\\.?[0-9]*)\\b"
		},
		{
			"comment": "Decimal",
			"name": "constant.numeric.cgpro",
			"match": "\\b(\\.[0-9]+)\\b"
		},
		{
			"comment": "Hexadecimal",
			"name": "constant.numeric.hex.cgpro",
			"match": "\\b(0x[0-9A-Fa-f]+)\\b"
		},
		{
			"comment": "Octal",
			"name": "constant.numeric.octal.cgpro",
			"match": "\\b(0o[0-7]+)\\b"
		},
		{
			"comment": "Binary",
			"name": "constant.numeric.binary.cgpro",
			"match": "\\b(0b[01]+)\\b"
		},
		{
			"comment": "Constant",
			"name": "constant.language.cgpro",
			"match": "\\b(false|true|null)\\b"
		},
		{
			"comment": "Datablock",
			"name": "string.datablock.cgpro",
			"begin": "\\[",
			"end": "\\]",
			"patterns": [
				{
					"name": "constant.character.escape.cgpro",
					"match": "\\\\."
				}
			]
		},
		{
			"comment": "String",
			"name": "string.quoted.double.cgpro",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.cgpro",
					"match": "\\\\."
				}
			]
		},
		{
			"comment": "Flow control unit keywords",
			"match": "\\b(?i)(?:and|by|end|elif|else|entry|exitif|external|for|forward|function|if|is|not|loop|or|procedure|return|spawn|stop|then|var|const|xor|while)\\b",
			"name": "keyword.control.cgpro"
		},
